{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConsulServiceOptions": {
    "ConsulAddress": "http://127.0.0.1:8500", //consul 服务器地址
    "ServiceName": "B", //服务名称，ApiGateway中配置的ServiceName对应这里
    "HealthCheck": "/Health", //健康检查地址
    "ServiceAddress": "http://127.0.0.1:5002" //webapi 地址
  },
  "AllowedHosts": "*"
}
